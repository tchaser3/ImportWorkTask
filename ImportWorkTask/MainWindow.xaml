<Window x:Class="ImportWorkTask.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        xmlns:local="clr-namespace:ImportWorkTask"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="525">
    <Grid>
        <Button x:Name="btnImportExcel" Content="Import Excel" HorizontalAlignment="Left" Margin="382,119,0,0" VerticalAlignment="Top" Width="125" Height="55" FontSize="18" Click="btnImportExcel_Click"/>
        <Button x:Name="btnProcess" Content="Process" HorizontalAlignment="Left" Margin="382,179,0,0" VerticalAlignment="Top" Width="125" Height="55" FontSize="18" Click="btnProcess_Click"/>
        <Button x:Name="btnClose" Content="Close" HorizontalAlignment="Left" Margin="382,239,0,0" VerticalAlignment="Top" Width="125" Height="55" FontSize="18" Click="btnClose_Click"/>
        <DataGrid x:Name="dgrExcel" HorizontalAlignment="Left" Height="504" Margin="10,55,0,0" VerticalAlignment="Top" Width="353"/>

    </Grid>
</Window>
